{
  "name": "gk-weather-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run test:scripts && npm run lint",
    "test:scripts": "react-scripts test --coverage --env=jsdom --setupFiles ./src/setupTests.js --watchAll=false",
    "test:scripts:watch": "react-scripts test --env=jsdom --setupFiles ./src/setupTests.js --coverage --watchAll=true",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "eject": "react-scripts eject"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/src/serviceWorker.js",
      "/src/index.jsx"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^4.5.3",
    "payload-action-creator": "0.0.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-async": "^10.0.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "styled-components": "^5.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.12.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "qa-utilities": "^1.1.0"
  }
}
